{
  "name": "@huma-finance/web-shared",
  "version": "0.0.61",
  "types": "./dist/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "start": "tsc -w --project ./tsconfig.build.json",
    "postbuild": "copyfiles -u 1 ./src/components/icons/*.svg dist",
    "build": "tsc --project ./tsconfig.build.json && tsc --project ./tsconfig.cjs.json",
    "lint": "eslint --max-warnings=0 \"src/**/*.{js,ts,tsx,graphql}\" \"tests/**/*.{js,ts,tsx,graphql}\"",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"**/*.{js,ts,tsx,graphql}\"",
    "test": "jest",
    "test:coverage": "jest --verbose --coverage",
    "test:badges": "yarn run test:coverage && yarn jest-coverage-badges"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "ethers": "^5.7.2",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "typescript": "^5.0.0",
    "utf8": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "git add . && lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,graphql}": [
      "eslint --fix --max-warnings=0",
      "prettier --write",
      "git add .",
      "yarn test --passWithNoTests"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.21",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^8.0.0",
    "jest": "^29.6.1",
    "jest-coverage-badges": "^1.1.2",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.7.1",
    "ts-jest": "^29.1.1"
  },
  "peerDependencies": {
    "ethers": "^5.6.1",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-react-ui": "^0.9.35",
    "@solana/wallet-adapter-wallets": "^0.19.32",
    "@solana/web3.js": "^1.95.3",
    "@solana/spl-token": "^0.4.8",
    "@stellar/freighter-api": "^3.0.0",
    "@huma-finance/soroban-credit-storage": "0.0.15",
    "@huma-finance/soroban-huma-config": "0.0.15",
    "@huma-finance/soroban-pool-storage": "0.0.15",
    "@huma-finance/soroban-sdk": "^0.0.15",
    "@huma-finance/soroban-tranche-vault": "0.0.15",
    "@stellar/stellar-sdk": "^12.3.0",
    "@coral-xyz/anchor": "^0.30.1",
    "lodash": "^4.17.21"
  },
  "optionalDependencies": {
    "encoding": "^0.1.13"
  },
  "gitHead": "27493bf7874db29d6a8a4792aac7b3d37ed28e15"
}
