{
  "name": "@huma-finance/shared",
  "version": "0.0.61",
  "types": "./dist/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "start": "tsc -w --project ./tsconfig.build.json",
    "postbuild": "copyfiles -u 1 ./src/components/icons/*.svg dist",
    "build": "tsc --project ./tsconfig.build.json && tsc --project ./tsconfig.cjs.json",
    "abi-type": "typechain --target ethers-v5 --out-dir src/abis/types \"./src/abis/**/*.json\"",
    "abi-type-v2": "typechain --target ethers-v5 --out-dir src/v2/abis/types \"./src/v2/abis/**/*.json\"",
    "lint": "eslint --max-warnings=0 \"src/**/*.{js,ts,tsx,graphql}\" \"tests/**/*.{js,ts,tsx,graphql}\"",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"**/*.{js,ts,tsx,graphql}\"",
    "test": "jest",
    "test:coverage": "jest --verbose --coverage",
    "test:badges": "yarn run test:coverage && yarn jest-coverage-badges"
  },
  "dependencies": {
    "@apollo/client": "^3.7.2",
    "@datadog/browser-rum": "^4.30.1",
    "@ethersproject/address": "^5.7.0",
    "@ethersproject/bignumber": "^5.6.0",
    "@ethersproject/constants": "^5.7.0",
    "@ethersproject/contracts": "^5.7.0",
    "@ethersproject/experimental": "^5.7.0",
    "@ethersproject/providers": "^5.6.0",
    "@ethersproject/units": "^5.6.0",
    "@types/utf8": "^3.0.1",
    "axios": "^1.4.0",
    "buffer": "^6.0.3",
    "encoding": "^0.1.13",
    "ethers": "^5.7.2",
    "graphql": "^16.6.0",
    "history": "^5.3.0",
    "moment": "^2.29.4",
    "qrcode": "^1.5.1",
    "query-string": "^7.1.1",
    "siwe": "^2.1.4",
    "tslib": "^2.5.0",
    "typescript": "^5.0.0",
    "utf8": "^3.0.0",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "git add . && lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,graphql}": [
      "eslint --fix --max-warnings=0",
      "prettier --write",
      "git add .",
      "yarn test --passWithNoTests"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.1.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^17.0.45",
    "@types/qrcode": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "copyfiles": "^2.4.1",
    "cypress": "^12.2.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "husky": "^8.0.0",
    "jest": "^29.6.1",
    "jest-coverage-badges": "^1.1.2",
    "jest-environment-jsdom": "^29.6.2",
    "lint-staged": "^10.5.4",
    "prettier": "^2.7.1",
    "ts-jest": "^29.1.1",
    "typechain": "^8.1.0",
    "url-loader": "^4.1.1"
  },
  "peerDependencies": {
    "@web3-react/types": "^8.2.0",
    "@web3-react/walletconnect": "^8.2.0",
    "@coral-xyz/anchor": "^0.30.1",
    "@solana/web3.js": "^1.95.3",
    "@solana/spl-token": "^0.4.8",
    "ethers": "^5.6.1",
    "graphql-request": "5.1.0"
  },
  "optionalDependencies": {
    "encoding": "^0.1.13"
  },
  "gitHead": "27493bf7874db29d6a8a4792aac7b3d37ed28e15"
}
