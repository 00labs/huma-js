{
  "name": "@huma-finance/sdk",
  "version": "0.0.47",
  "types": "./dist/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/index.js",
  "files": [
    "dist",
    "getting-started.md",
    "API.md"
  ],
  "scripts": {
    "start": "tsc -w",
    "graphql": "graphql-codegen",
    "generateDocs": "jsdoc2md --files \"src/**/*.ts\" --configure ./jsdoc2md.json > API.md",
    "build": "tsc --project ./tsconfig.build.json && tsc --project ./tsconfig.cjs.json",
    "lint": "eslint --max-warnings=0 \"src/**/*.{js,ts,tsx,graphql}\" \"tests/**/*.{js,ts,tsx,graphql}\"",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"src/**/*.{js,ts,tsx,graphql}\"",
    "test": "jest",
    "test:coverage": "jest --verbose --coverage",
    "test:badges": "yarn run test:coverage && yarn jest-coverage-badges"
  },
  "dependencies": {
    "@bundlr-network/client": "0.11.17",
    "@ethersproject/address": "^5.7.0",
    "@ethersproject/bignumber": "^5.6.0",
    "@ethersproject/constants": "^5.7.0",
    "@ethersproject/contracts": "^5.7.0",
    "@ethersproject/providers": "^5.6.0",
    "@huma-finance/shared": "^0.0.47",
    "axios": "^1.4.0",
    "bignumber.js": "^9.1.1",
    "ethers": "^5.7.2",
    "graphql-request": "5.1.0",
    "react": "^18.2.0",
    "tslib": "^2.5.0",
    "typescript": "^4.8.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "git add . && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,graphql}": [
      "yarn generateDocs",
      "eslint --fix --max-warnings=0",
      "prettier --write",
      "yarn test:badges",
      "git add ."
    ],
    "tests/**/*.{js,ts}": [
      "eslint --fix --max-warnings=0",
      "prettier --write",
      "yarn test:badges",
      "git add ."
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/typescript": "^3.0.4",
    "@graphql-codegen/typescript-graphql-request": "^4.5.9",
    "@testing-library/react": "^14.0.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^29.6.1",
    "jest-coverage-badges": "^1.1.2",
    "jest-environment-jsdom": "^29.6.1",
    "jsdoc": "^4.0.2",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^8.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.7.1",
    "ts-jest": "^29.1.1"
  },
  "peerDependencies": {
    "ethers": "^5.6.1",
    "react": ">=16.8.0"
  },
  "optionalDependencies": {
    "encoding": "^0.1.13"
  },
  "gitHead": "27493bf7874db29d6a8a4792aac7b3d37ed28e15"
}
